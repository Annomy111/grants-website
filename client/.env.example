# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_project_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration  
# For local development use: http://localhost:8888/.netlify/functions
# For production use: /.netlify/functions
REACT_APP_API_URL=http://localhost:8888/.netlify/functions

# Feature Flags
# Set to true to use static JSON files instead of API
REACT_APP_USE_STATIC_DATA=false

# Google Gemini API (for AI chat features)
REACT_APP_GOOGLE_GEMINI_API_KEY=your_gemini_api_key_optional

# Environment
REACT_APP_ENVIRONMENT=development